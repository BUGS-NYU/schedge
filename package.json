{
  "name": "schedge",
  "scripts": {
    "dev": "next dev -p 9100",
    "export": "next build && next export -o src/main/resources/next",
    "start": "next start",
    "lint": "next lint",
    "deploy": "git push -f origin main:production"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.14",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-preset-env": {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "nesting-rules": true,
          "custom-properties": false
        }
      }
    }
  }
}
